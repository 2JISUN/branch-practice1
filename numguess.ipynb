   "source": [
    "from random import randint\n",
    "answer = randint(1,100)\n",
    "print(answer)\n",
    "\n",
    "while True:\n",
    "    guess = int(input(\"Guess the num(1-100): \"))\n",
    "    if answer == guess:\n",
    "        print(\"good\")\n",
    "        break\n",
    "    elif answer>guess:\n",
    "        print(\"up\")\n",
    "    elif answer<guess:\n",
    "        print(\"down\")"
   ]
  }
